{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "es2015"
    ],
    "sourceMap": true,
    "inlineSources": false,
    "declaration": true,
    "declarationMap": true,
    "outDir": "./lib",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "./node_modules",
    "./lib/**/*"
  ]
}
